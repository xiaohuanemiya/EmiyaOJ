{
  "name": "emiyaoj-user-frontend",
  "version": "1.0.0",
  "type": "module",
  "description": "EmiyaOJ Online Judge System - User Frontend",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,ts,vue,scss}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "pinia": "^2.3.0",
    "axios": "^1.7.9",
    "element-plus": "^2.9.1",
    "@element-plus/icons-vue": "^2.3.1",
    "monaco-editor": "^0.45.0",
    "markdown-it": "^14.1.0",
    "highlight.js": "^11.10.0",
    "dayjs": "^1.11.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "vite": "^6.0.5",
    "vue-tsc": "^2.1.10",
    "typescript": "^5.7.2",
    "sass": "^1.83.0",
    "sass-loader": "^16.0.4",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.31.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "prettier": "^3.4.2",
    "vitest": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "jsdom": "^24.1.3",
    "unplugin-auto-import": "^0.17.8",
    "unplugin-vue-components": "^0.26.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
